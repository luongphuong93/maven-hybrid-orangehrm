<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >

<suite name="OrangeHRM - UI Automation Test" verbose="1" >
    <listeners>
        <listener class-name="core.MethodListener" />
    </listeners>

    <parameter name="appUrl" value="http://localhost/orangehrm/web/index.php/auth/login" />

    <test name="Run on FIREFOX browser" >
        <parameter name="browser" value="Firefox" />
        <classes>
            <class name="com.orangehrm.Level_13_Logging" />
        </classes>
    </test>

</suite>